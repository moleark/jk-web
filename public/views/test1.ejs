<style>
    p {margin-bottom:0.7em;}
    .title-top-line {
        border-top-width: 0.15em;
        border-top-style: solid; 
        padding-top: 0.1rem;
    }
    .title-top-line-right-angle>:first-child {
        line-height: 1.4em; 
        padding: 0.3em 3em 0.3em 0.8em; 
        border-bottom-right-radius: 3em;
    }
    .first-char-large::first-letter {
        font-size: 2em;
        letter-spacing: 0.1em;
    }
    .firt-line-height-small::first-line {
        line-height: 0;
    }
</style>

<%
function product(p) {
    let url1 = p[0];
    let url2 = p[1];
    let no = p[2];
    let url3 = p[3]; 
    let cas = p[4];
    let caption = p[5];
    if (url1.length<10) {
        url1 = 'https://c.jkchemical.com/res/' + url1 + '.png';
    }
    if (url2.length<10) {
        url2 = 'https://www.jkchemical.com/CH/products/' + url2 + no + '.html';
    }
    if (url3.length<15) {
        url3 = 'https://www.jkchemical.com/CH/products/search/' + 'url3/' + cas+ '.html';
    }
%>
<div class="col py-1 py-sm-2 border-bottom border-muted" style="line-height:1.5em; ">
    <img class="float-right w-25 border p-2 ml-3" src="<%=url1%>">
    <a href="<%=url2%>" class="text-decoration-none">
        <div style="color: tomato; font-weight:600;"><%=no%></div>
    </a>
    <a href="<%=url3%>" class="text-decoration-none">
        <div style="color: tomato;"><%=cas%></div>
    </a>
    <div class="text-dark">
        <b><%-caption%></b>
    </div>
</div>
<%
}

function products(arr) {
    %> <div class="row row-cols-1 row-cols-lg-2 mb-3"> <%
    arr.forEach(element => {
        product(element)
    });
    %> </div> <%
}

function subtitle(text) {
    %>
        <div class="title-top-line border-info text-primary d-flex clearfix">
            <div class="border-info py-2 mb-0 pl-2 pr-4 d-flex align-items-center">
                <i class="mr-2 fa fa-circle" style="font-size: 60%;"></i>
                <div><b><%-text%></b></div>
            </div>
        </div>
    <%
}

