<%
function product(p) {
    let url = p[0];
    let no = p[1];
    let caption = p[2];
    if (url.length<100) {
        url = 'https://www.jkchemical.com/CH/products/search/' + url + no + '.html';
    }
%>
<div class="col-sm py-1  py-sm-3  border-bottom border-muted">
     <a href="<%=url%>" class="text-decoration-none">
         <div style="line-height:1.5em;">
             <div class="text-info mb-1" style="font-weight:600;"><%=no%></div>
             <div class="text-dark"><%-caption%></div>
         </div>
     </a>
</div>
<%
}

function products(arr) {
    %> <div class="container">
        <div class="row row-cols-1 row-cols-sm-1 row-cols-md-3"> <%
    arr.forEach(element => {
        product(element)
    });
    %> </div></div> <%
}

function subtitle1(text) {
    %>
        <div class="h4 text-info d-flex clearfix">
            <div class="py-2 mb-0 pr-4 d-flex align-items-center">
                <div><b><%-text%></b></div>
            </div>
        </div>
    <%
}

function subtitle2(text) {
    %>
        <div class="h5 text-dark d-flex clearfix">
            <div class="py-2 mb-0 pl-2 pr-4 d-flex align-items-center">
                <li style="list-style:square;"><b><%-text%></b></li>
            </div>
        </div>
    <%
}

function subtitle3(text) {
    %>
        <div class="border-bottom border-muted my-2" >
