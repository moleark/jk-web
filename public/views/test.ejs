function product(p) {
    let url = p[0];
    let no = p[1];
    let caption = p[2];
    let pack = p[3];
    if (url.length<10) {
        url = 'https://www.jkchemical.com/CH/products/' + url + no + '.html';
    }
%>
<div class="col-sm py-3  border-bottom border-muted">
<a href="<%=url%>" class="text-decoration-none">
<div style="line-height:2.0em; " class="text-dark">
<div style="color: tomato; font-weight:600;"><%=no%></div>
<div><b class="text-dark"><%=caption%></b></div>
<div><%=pack%></div>
</div>
</a>
</div>
<%
}

function products(arr) {
    %> <div class="row mt-2 row-cols-1 row-cols-sm-2 row-cols-md-3"> <%
    arr.forEach(element => {
        product(element)
    });
    %> </div> <%
}

function subtitle(text) {
    %>
        <div class="mt-5 mb-2 text-info d-flex align-items-center h5">
            <div class="mr-3 d-flex align-items-center" style="font-size: 60%;"><i class="text-success fa fa-circle"></i></div>
            <div><b><%=text%></b></div>
        </div> 
    <%
}

